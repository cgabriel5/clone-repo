{
	"name": "clone-repo",
	"version": "0.0.1",
	"description": "Clones a public GitHub, GitLab, or BitBucket repo from the CLI using a simple, unified API.",
	"main": "./src/index.js",
	"bin": {
		"clone-repo": "./src/index.js"
	},
	"bugs": {
		"url": "https://github.com/cgabriel5/clone-repo/issues"
	},
	"homepage": "https://github.com/cgabriel5/clone-repo#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cgabriel5/clone-repo.git"
	},
	"scripts": {
		"pretty": "prettier-cli-watcher --configpath='/configs/prettier.config.json'",
		"lint": "cross-env ESLINT_CFS_VERBOSE=true ./node_modules/eslint/bin/eslint.js -c='configs/eslint.config.js' --ignore-path='configs/eslintignore' --format='simple' '**/*.{js,jsx}'; exit 0"
	},
	"dependencies": {
		"chalk": "^2.4.1",
		"cross-spawn-with-kill": "^1.0.0",
		"fancy-log": "^1.3.3",
		"minimist": "^1.2.0"
	},
	"author": "Carlos Gabriel",
	"license": "MIT",
	"keywords": [
		"git",
		"clone",
		"public",
		"github",
		"gitlab",
		"bitbucket",
		"repo",
		"repository",
		"https",
		"ssh"
	],
	"engines": {
		"node": ">=8.14.0",
		"npm": ">=6.4.1"
	},
	"devDependencies": {
		"cross-env": "^5.2.0",
		"eslint": "^5.12.0",
		"eslint-config-simple": "^0.0.2",
		"eslint-formatter-simple": "^0.0.3",
		"prettier-cli-watcher": "^0.3.1"
	}
}
